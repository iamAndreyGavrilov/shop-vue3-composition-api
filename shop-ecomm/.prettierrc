{
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "bracketLine": false,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "lf",
  "insertPragma": false,
  "printWidth": 100,
  "proseWrap": "preserve",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "quoteProps": "consistent",
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": false,
  "vueIndentScriptAndStyle": false
}
